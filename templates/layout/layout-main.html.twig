{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('css/main') }}
{% endblock %}

{% block body %}
    {% include 'layout/_nav.html.twig' only %}

    {% block content %}{% endblock %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('js/main') }}
    <script src="https://use.fontawesome.com/releases/v5.15.1/js/all.js"></script>
    <script src="https://use.fontawesome.com/releases/v5.15.1/js/v4-shims.js"></script>
{% endblock %}